{
  "name": "zzzync",
  "version": "0.0.0",
  "description": "replication protocol over IPLD, IPNS, and IPFS Provider Records",
  "type": "module",
  "files": [
    "dist/src",
    "!**/*.tsbuildinfo"
  ],
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "main": "index.js",
  "scripts": {
    "clean": "aegir clean",
    "lint": "aegir lint",
    "dep-check": "aegir dep-check",
    "build": "aegir build",
    "docs": "aegir docs",
    "test": "aegir test",
    "test:chrome": "aegir test -t browser --cov",
    "test:chrome-webworker": "aegir test -t webworker",
    "test:firefox": "aegir test -t browser -- --browser firefox",
    "test:firefox-webworker": "aegir test -t webworker -- --browser firefox",
    "test:node": "aegir test -t node --cov",
    "test:electron-main": "aegir test -t electron-main",
    "release": "aegir release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tabcat/zzzync.git"
  },
  "keywords": [
    "IPFS",
    "IPLD",
    "IPNS",
    "DHT",
    "replication"
  ],
  "author": "tabcat <tabcat00@proton.me>",
  "license": "Apache-2.0 OR MIT",
  "bugs": {
    "url": "https://github.com/tabcat/zzzync/issues"
  },
  "homepage": "https://github.com/tabcat/zzzync#readme",
  "devDependencies": {
    "@chainsafe/libp2p-noise": "^11.0.4",
    "@chainsafe/libp2p-yamux": "^3.0.7",
    "@libp2p/peer-id-factory": "^2.0.3",
    "@libp2p/tcp": "^6.1.5",
    "aegir": "^39.0.5",
    "go-ipfs": "^0.20.0",
    "helia": "^1.0.4",
    "ipfsd-ctl": "^13.0.0",
    "kubo-rpc-client": "^3.0.1",
    "merge-options": "^3.0.4",
    "wherearewe": "^2.0.1"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts"
  },
  "dependencies": {
    "@helia/interface": "^1.0.0",
    "@helia/ipns": "^1.1.1",
    "@libp2p/interface-dht": "^2.0.3",
    "@libp2p/interface-peer-id": "^2.0.2",
    "@libp2p/kad-dht": "^8.0.6",
    "@libp2p/webrtc-star": "^7.0.0",
    "datastore-core": "^9.1.1",
    "interface-store": "^5.1.0",
    "it-all": "^3.0.1",
    "libp2p": "^0.44.0",
    "libp2p-crypto": "^0.21.2",
    "multiformats": "^11.0.2",
    "w3name": "^1.0.8"
  },
  "browser": {
    "./dist/src/utils/libp2p.js": "./dist/src/utils/libp2p.browser.js"
  }
}
