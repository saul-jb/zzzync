{
  "name": "@tabcat/zzzync",
  "version": "1.0.0",
  "description": "replication protocol over IPLD, IPNS, and IPFS Provider Records",
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ],
      "src/*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ]
    }
  },
  "files": [
    "dist/src",
    "!**/*.tsbuildinfo"
  ],
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./advertisers/dht": {
      "types": "./dist/src/advertisers/dht.d.ts",
      "import": "./dist/src/advertisers/dht.js"
    },
    "./namers/w3name": {
      "types": "./dist/src/namers/w3name.d.ts",
      "import": "./dist/src/namers/w3name.js"
    }
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "aegir clean",
    "lint": "aegir lint",
    "dep-check": "aegir dep-check",
    "build": "aegir build",
    "docs": "aegir docs",
    "test": "aegir test",
    "test:chrome": "aegir test -t browser --cov",
    "test:chrome-webworker": "aegir test -t webworker",
    "test:firefox": "aegir test -t browser -- --browser firefox",
    "test:firefox-webworker": "aegir test -t webworker -- --browser firefox",
    "test:node": "aegir test -t node --cov",
    "test:electron-main": "aegir test -t electron-main"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tabcat/zzzync.git"
  },
  "keywords": [
    "IPFS",
    "IPLD",
    "IPNS",
    "DHT",
    "replication"
  ],
  "author": "tabcat <tabcat00@proton.me>",
  "license": "Apache-2.0 OR MIT",
  "bugs": {
    "url": "https://github.com/tabcat/zzzync/issues"
  },
  "homepage": "https://github.com/tabcat/zzzync#readme",
  "devDependencies": {
    "@chainsafe/libp2p-noise": "^12.0.1",
    "@chainsafe/libp2p-yamux": "^4.0.2",
    "@libp2p/interface-libp2p": "^3.2.0",
    "@libp2p/peer-id-factory": "^2.0.3",
    "@libp2p/tcp": "^7.0.1",
    "@libp2p/webrtc": "^2.0.6",
    "aegir": "^39.0.5",
    "blockstore-core": "^4.2.0",
    "go-ipfs": "^0.20.0",
    "helia": "^1.2.1",
    "ipfsd-ctl": "^13.0.0",
    "ipns": "^6.0.0",
    "kubo-rpc-client": "^3.0.1",
    "merge-options": "^3.0.4",
    "wherearewe": "^2.0.1"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts"
  },
  "dependencies": {
    "@helia/interface": "^1.1.1",
    "@helia/ipns": "^1.1.3",
    "@libp2p/interface-peer-id": "^2.0.2",
    "@libp2p/kad-dht": "^9.3.6",
    "@libp2p/websockets": "^6.0.1",
    "@multiformats/multiaddr": "^12.1.3",
    "datastore-core": "^9.1.1",
    "interface-datastore": "^8.2.2",
    "interface-store": "^5.1.0",
    "libp2p": "^0.45.4",
    "libp2p-crypto": "^0.21.2",
    "multiformats": "^11.0.2",
    "w3name": "^1.0.8"
  },
  "browser": {
    "./dist/test/utils/create-libp2p.js": "./dist/test/utils/create-libp2p.browser.js"
  }
}
