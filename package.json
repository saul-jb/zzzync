{
  "name": "zzzync",
  "version": "0.0.0",
  "description": "replication protocol over IPLD, IPNS, and IPFS Provider Records",
  "type": "module",
  "files": [
    "dist/src",
    "!**/*.tsbuildinfo"
  ],
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "main": "index.js",
  "scripts": {
    "clean": "aegir clean",
    "lint": "aegir lint",
    "dep-check": "aegir dep-check",
    "build": "aegir build",
    "docs": "aegir docs",
    "test": "aegir test",
    "test:chrome": "aegir test -t browser --cov",
    "test:chrome-webworker": "aegir test -t webworker",
    "test:firefox": "aegir test -t browser -- --browser firefox",
    "test:firefox-webworker": "aegir test -t webworker -- --browser firefox",
    "test:node": "aegir test -t node --cov",
    "test:electron-main": "aegir test -t electron-main",
    "release": "aegir release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tabcat/zzzync.git"
  },
  "keywords": [
    "IPFS",
    "IPLD",
    "IPNS",
    "DHT",
    "replication"
  ],
  "author": "tabcat <tabcat00@proton.me>",
  "license": "Apache-2.0 OR MIT",
  "bugs": {
    "url": "https://github.com/tabcat/zzzync/issues"
  },
  "homepage": "https://github.com/tabcat/zzzync#readme",
  "devDependencies": {
    "aegir": "^39.0.5"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts"
  },
  "dependencies": {
    "@helia/ipns": "^1.1.1",
    "@libp2p/interface-dht": "^2.0.3",
    "@libp2p/interface-peer-id": "^2.0.2",
    "@libp2p/kad-dht": "^9.2.0",
    "datastore-core": "^9.1.1",
    "helia": "^1.0.4",
    "ipns": "^6.0.0",
    "it-all": "^3.0.1",
    "libp2p": "^0.44.0",
    "multiformats": "^11.0.2"
  }
}
